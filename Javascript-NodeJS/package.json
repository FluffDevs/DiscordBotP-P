{
  "name": "discord-bot-pp",
  "version": "0.1.0",
  "description": "Squelette de bot Discord avec dotenv (fr)",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --watch src --exec node src/index.js",
    "lint": "eslint . --ext .js",
    "test": "node test/test.js",
    "deploy-commands": "node src/deploy-commands.js",
    "deploy-if-ready": "node scripts/deploy-if-ready.js"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "dependencies": {
    "@discordjs/rest": ">=2.6.0",
    "discord-api-types": ">=0.38.33",
    "discord.js": ">=14.12.0",
    "dotenv": ">=16.0.0",
    "puppeteer": ">=21.3.0"
  },
  "devDependencies": {
    "eslint": ">=9.39.1",
    "nodemon": ">=3.1.11"
  }
}
